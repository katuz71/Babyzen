{
    "java.compile.nullAnalysis.mode": "automatic",
    "java.configuration.updateBuildConfiguration": "interactive",
    "git.ignoreLimitWarning": true,
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "typescript.tsserver.exclude": [
        "**/node_modules/**",
        "supabase/functions/**"
    ],
    "deno.enable": false,
    "deno.enablePaths": [
        "supabase/functions"
    ],
    "deno.config": "./supabase/functions/deno.json"
}